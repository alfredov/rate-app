{
  "name": "rate-app",
  "version": "1.0.0",
  "main": "src/index",
  "license": "MIT",
  "scripts": {
    "webpack-dev-server": "webpack-dev-server --host 0.0.0.0",
    "webpack": "webpack",
    "debug": "node --inspect --inspect-brk ./node_modules/webpack/bin/webpack.js",
    "prod": "npm run webpack -- --env.mode production && npm run sh:copy-public-to-build && npm run sh:remove-dist-coverage",
    "start": "npm run webpack-dev-server -- --env.mode development --hot",
    "prod:analyze": "npm run prod -- --env.presets analyze",
    "prod:compress": "npm run prod -- --env.presets compress",
    "prod:debug": "npm run debug -- --env.mode production",
    "dev:debug": "npm run debug -- --env.mode development",
    "lint": "tslint -p ./tsconfig.json",
    "sh:copy-public-to-build": "bash ./tools/bash/copy-public-to-build.sh",
    "sh:remove-dist-coverage": "bash ./tools/bash/remove-dist-coverage.sh",
    "storybook": "start-storybook -p 6006 -c ./config/storybook",
    "build-storybook": "build-storybook",
    "build:esnext": "yarn install && mkdir -p ./dist/esnext && rm -rf ./dist/esnext && tsc -p ./config/tsconfig.esnext.json",
    "extract:i18n:base": "yarn build:esnext && yarn format-message extract './dist/esnext/**/*.js' -g underscored_crc32",
    "extract:i18n:lang": "sh -c 'yarn extract:i18n:base -l ${0} -o ./src/core/i18n/${0}.json'",
    "extract:i18n": "yarn extract:i18n:lang en && bash ./tools/bash/remove-dist-esnext.sh",
    "test": "yarn install && mkdir -p ./dist/coverage && node --expose-gc ./node_modules/.bin/jest --config=./config/jest/index.js -u --json --outputFile=./dist/coverage/test-results.json --logHeapUsage",
    "test:w": "yarn test --watchAll",
    "jest": "jest --config=./config/jest/index.js",
    "ts-coverage": "mkdir -p ./dist/coverage-ts && typescript-coverage-report -o ./dist/coverage-ts -t 98"
  },
  "typeCoverage": {
    "atLeast": 98,
    "ignoreCatch": true
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@storybook/addon-a11y": "^6.1.11",
    "@storybook/addon-actions": "^6.1.11",
    "@storybook/addon-backgrounds": "^6.1.11",
    "@storybook/addon-controls": "^6.1.11",
    "@storybook/addon-docs": "^6.1.11",
    "@storybook/addon-essentials": "^6.1.11",
    "@storybook/addon-jest": "^6.1.11",
    "@storybook/addon-knobs": "^6.1.11",
    "@storybook/addon-links": "^6.1.11",
    "@storybook/addon-viewport": "^6.1.11",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^6.1.11",
    "@types/enzyme": "3.10.7",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/jest": "26.0.13",
    "@types/node": "^14.14.16",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.14",
    "@types/react-router-dom": "^5.1.6",
    "@types/storybook-readme": "^5.0.4",
    "babel-loader": "^8.2.2",
    "compression-webpack-plugin": "^7.1.1",
    "css-loader": "^5.0.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.4",
    "format-message-cli": "^6.2.3",
    "html-webpack-plugin": "^4.5.0",
    "jest": "26.4.2",
    "mini-css-extract-plugin": "^1.3.3",
    "redux-logger": "^3.0.6",
    "storybook-readme": "^5.0.9",
    "style-loader": "^2.0.0",
    "ts-jest": "26.3.0",
    "ts-loader": "^8.0.12",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3",
    "typescript-coverage-report": "^0.4.0",
    "webpack": "4.8.0",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-cli": "2.1.3",
    "webpack-dev-server": "3.1.4",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "@emotion/react": "^11.1.4",
    "connected-react-router": "^6.8.0",
    "dotenv": "^8.2.0",
    "format-message": "^6.2.3",
    "format-message-generate-id": "^6.2.3",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-observable": "^1.2.0",
    "rxjs": "^6.6.3",
    "typesafe-actions": "^5.1.0"
  }
}
